{
  "name": "react-highcharts-wrapper",
  "version": "0.0.22",
  "description": "A React wrapper for building Highcharts components, using the Provider pattern to supply Highcharts the right way.",
  "main": "lib/ReactHighchartsWrapper.js",
  "license": "MIT",
  "repository": "git+https://github.com/beestripes/react-highcharts-wrapper.git",
  "keywords": [
    "highcharts",
    "highcharts-hoc",
    "highcharts-provider",
    "highcharts-wrapper",
    "highcharts-react-wrapper",
    "react-highcharts",
    "react-highcharts-wrapper",
    "highcharts-react",
    "react-component",
    "react-provider",
    "react-hoc",
    "dataviz",
    "react",
    "react highcharts",
    "highcharts react",
    "react-component"
  ],
  "author": "Elise Chant <elise.chant@gmail.com>",
  "bugs": {
    "url": "https://github.com/beestripes/react-highcharts-wrapper/issues"
  },
  "homepage": "https://react-highcharts-wrapper.firebaseapp.com",
  "scripts": {
    "clean:lib": "rm -rf lib",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "prebuild:lib": "yarn clean:lib",
    "build:lib": "babel src/ --out-dir lib --source-maps --ignore test.js,example/,index.js",
    "test": "react-scripts test --env=jsdom",
    "test:ci": "yarn test -- --coverage --runInBand",
    "eject": "react-scripts eject",
    "prepublishOnly": "yarn build:lib",
    "start:storybook": "yarn run build:lib && start-storybook -p 9009 -c examples/.storybook -s examples/public",
    "build:storybook": "yarn run build:lib && build-storybook -c examples/.storybook -s examples/public",
    "predeploy": "yarn build:storybook",
    "deploy": "firebase deploy"
  },
  "dependencies": {},
  "peerDependencies": {
    "highcharts": ">=5",
    "prop-types": ">=15",
    "react": ">=15",
    "react-dom": ">=15"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.6",
    "@storybook/addon-links": "^3.2.6",
    "@storybook/react": "^3.2.8",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "codecov": "^2.3.0",
    "enzyme": "^2.9.1",
    "expect": "^1.20.2",
    "highcharts": ">=5",
    "jsdom": "^11.2.0",
    "prop-types": ">=15",
    "react": ">=15",
    "react-dom": ">=15",
    "react-scripts": "1.0.12",
    "react-test-renderer": "^15.6.1"
  }
}
