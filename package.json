    "test": "react-scripts test --env=jsdom",
    "test:ci": "yarn run test -- --coverage --runInBand",
    "eject": "react-scripts eject",
    "prepublishOnly": "yarn run build:lib"
  },
  "dependencies": {
    "prop-types": "^15.5.10"
