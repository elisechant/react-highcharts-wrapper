{
  "name": "react-highcharts-wrapper",
  "version": "0.0.11",
  "description": "A React wrapper for building Highcharts components.",
  "main": "lib/ReactHighchartsWrapper.js",
  "license": "MIT",
  "repository": "git+https://github.com/beestripes/react-highcharts-wrapper.git",
  "keywords": [
    "highcharts",
    "highcharts-hoc",
    "highcharts-provider",
    "highcharts-wrapper",
