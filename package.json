    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^2.9.1",
    "expect": "^1.20.2",
    "highcharts": ">=5",
    "jsdom": "^11.2.0",
    "react": ">=15",
    "react-dom": ">=15",
    "react-scripts": "1.0.12",
    "react-test-renderer": "^15.6.1"
  }
